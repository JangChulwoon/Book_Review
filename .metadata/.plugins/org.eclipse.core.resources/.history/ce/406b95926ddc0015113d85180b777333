connect 이름 : jsp
비밀번호  : passwd

//스키마 생성
CREATE SCHEMA `notfound` DEFAULT CHARACTER SET utf8 ;

// 테이블 생성
CREATE TABLE `user` (
  `email` varchar(40) NOT NULL,
  `pass` varchar(40) DEFAULT NULL,
  `name` varchar(40) DEFAULT NULL,
  PRIMARY KEY (`email`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


CREATE TABLE `reple` (
  `num` int(11) DEFAULT NULL,
  `id` varchar(45) DEFAULT NULL,
  `content` longblob,
  `date` datetime DEFAULT NULL,
  `count` int(11) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`count`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;



CREATE TABLE `board` (
  `num` int(11) NOT NULL AUTO_INCREMENT COMMENT '번호',
  `subject` varchar(250) NOT NULL COMMENT '제목',
  `writer` varchar(50) NOT NULL COMMENT '작성자',
  `contents` longblob COMMENT '내용',
  `reg_date` varchar(50) DEFAULT NULL COMMENT '등록 일시',
  `bookname` varchar(250) DEFAULT NULL,
  `author` varchar(250) DEFAULT NULL,
  `publisher` varchar(250) DEFAULT NULL,
  `publication_date` varchar(50) DEFAULT NULL,
  `book_img` longblob,
  `description` longblob,
  PRIMARY KEY (`num`),
  UNIQUE KEY `NUM` (`num`)
) ENGINE=MyISAM AUTO_INCREMENT=493 DEFAULT CHARSET=utf8 COMMENT='게시판';
